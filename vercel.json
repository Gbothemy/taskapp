{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/auth",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/tasks/(.*)",
      "dest": "/api/tasks.js"
    },
    {
      "src": "/api/tasks",
      "dest": "/api/tasks.js"
    },
    {
      "src": "/api/users/(.*)",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/users",
      "dest": "/api/users.js"
    },
    {
      "src": "/api/payments/(.*)",
      "dest": "/api/payments.js"
    },
    {
      "src": "/api/payments",
      "dest": "/api/payments.js"
    },
    {
      "src": "/api/admin/(.*)",
      "dest": "/api/admin.js"
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/health.js"
    }
  ]
}